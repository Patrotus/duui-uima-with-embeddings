<?xml version="1.0" encoding="UTF-8"?>
<typeSystemDescription xmlns="http://uima.apache.org/resourceSpecifier">
    <types>
        <typeDescription>
            <name>org.texttechnologylab.annotation.type.Image</name>
            <description/>
            <supertypeName>uima.tcas.Annotation</supertypeName>
            <features>
                <featureDescription>
                    <name>src</name>
                    <description/>
                    <rangeTypeName>uima.cas.String</rangeTypeName>
                </featureDescription>
                <featureDescription>
                    <name>width</name>
                    <description/>
                    <rangeTypeName>uima.cas.Integer</rangeTypeName>
                </featureDescription>
                <featureDescription>
                    <name>height</name>
                    <description/>
                    <rangeTypeName>uima.cas.Integer</rangeTypeName>
                </featureDescription>
                <featureDescription>
                    <name>mimetype</name>
                    <description/>
                    <rangeTypeName>uima.cas.String</rangeTypeName>
                </featureDescription>
            </features>
        </typeDescription>

        <typeDescription>
            <name>org.texttechnologylab.annotation.type.Video</name>
            <description/>
            <supertypeName>uima.tcas.Annotation</supertypeName>
            <features>
                <featureDescription>
                    <name>src</name>
                    <description>path to the video or base64 value</description>
                    <rangeTypeName>uima.cas.String</rangeTypeName>
                </featureDescription>
                <featureDescription>
                    <name>length</name>
                    <description>Length of the video in seconds</description>
                    <rangeTypeName>uima.cas.Double</rangeTypeName>
                </featureDescription>
                <featureDescription>
                    <name>fps</name>
                    <description>Video fps</description>
                    <rangeTypeName>uima.cas.Double</rangeTypeName>
                </featureDescription>
            </features>
        </typeDescription>

        <typeDescription>
            <name>org.texttechnologylab.annotation.type.Audio</name>
            <description/>
            <supertypeName>uima.tcas.Annotation</supertypeName>
            <features>
                <featureDescription>
                    <name>src</name>
                    <description>path to the audio or base64 value</description>
                    <rangeTypeName>uima.cas.String</rangeTypeName>
                </featureDescription>
            </features>
        </typeDescription>

        <typeDescription>
            <name>org.texttechnologylab.annotation.type.Text</name>
            <description/>
            <supertypeName>uima.tcas.Annotation</supertypeName>
            <features>
                <featureDescription>
                    <name>text</name>
                    <description>Content of a text message</description>
                    <rangeTypeName>uima.cas.String</rangeTypeName>
                </featureDescription>
            </features>
        </typeDescription>

        <typeDescription>
            <name>org.texttechnologylab.annotation.embeddings.Embedding</name>
            <description>Basic type for all kinds of embeddings</description>
            <supertypeName>uima.tcas.Annotation</supertypeName>
            <features>
                <featureDescription>
                    <name>embedding</name>
                    <description>Represents embeddings as a vector</description>
                    <rangeTypeName>uima.cas.FloatArray</rangeTypeName>
                </featureDescription>
                <featureDescription>
                    <name>shape</name>
                    <description>Shape of the returned tensor</description>
                    <rangeTypeName>uima.cas.IntegerArray</rangeTypeName>
                </featureDescription>
            </features>
        </typeDescription>

        <typeDescription>
            <name>org.texttechnologylab.annotation.embeddings.TextEmbedding</name>
            <description>Text-Embedding</description>
            <supertypeName>org.texttechnologylab.annotation.embeddings.Embedding</supertypeName>
        </typeDescription>

        <typeDescription>
            <name>org.texttechnologylab.annotation.embeddings.ImageEmbedding</name>
            <description>Bild-Embedding</description>
            <supertypeName>org.texttechnologylab.annotation.embeddings.Embedding</supertypeName>
        </typeDescription>

        <typeDescription>
            <name>org.texttechnologylab.annotation.embeddings.VideoEmbedding</name>
            <description>Video-Embedding</description>
            <supertypeName>org.texttechnologylab.annotation.embeddings.Embedding</supertypeName>
        </typeDescription>

        <typeDescription>
            <name>org.texttechnologylab.annotation.embeddings.AudioEmbedding</name>
            <description>Audio-Embedding</description>
            <supertypeName>org.texttechnologylab.annotation.embeddings.Embedding</supertypeName>
        </typeDescription>

    </types>
</typeSystemDescription>
